{
    "name": "aura/auth",
    "type": "library",
    "description": "Aura Auth provides a unified interface to authenticate a user with local or remote authentication systems.",
    "keywords": [
        "auth",
        "authenticate",
        "authentication"
    ],
    "homepage": "http://auraphp.com/Aura.Auth",
    "version": "2.0.0-dev",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Aura Contributors",
            "homepage": "https://github.com/auraphp/Aura.Auth/contributors"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "suggest": {
        "ircmaxell/password-compat": "Provides the more-secure password_hash() functionality for PHP versions earlier than 5.5"
    },
    "autoload": {
        "psr-4": {
            "Aura\\Auth\\": "src/",
            "Aura\\Auth\\_Config\\": "config/"
        }
    },
    "extra": {
        "aura": {
            "type": "library",
            "config": {
                "common": "Aura\\Auth\\_Config\\Common"
            }
        },
        "branch-alias": {
            "dev-develop-2": "2.0.x-dev"
        }
    }
}
