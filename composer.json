{
    "name": "aura/auth",
    "type": "library",
    "description": "Aura Auth provides a unified interface to authenticate a user with local or remote authentication systems.",
    "keywords": [
        "auth",
        "authenticate",
        "authentication"
    ],
    "homepage": "http://auraphp.com/Aura.Auth",
    "version": "2.0.0-dev",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Aura Contributors",
            "homepage": "https://github.com/auraphp/Aura.Auth/contributors"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "ircmaxell/password-compat": "~1.0.0"
    },
    "suggest": {
        "ircmaxell/password-compat": "A compatibility library for the proposed simplified password hashing algorithm: https://wiki.php.net/rfc/password_hash"
    },
    "autoload": {
        "psr-4": {
            "Aura\\Auth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aura\\Filter\\": "tests"
        }
    },
    "extra": {
        "aura": {
            "type": "library",
            "config": {
            }
        },
        "branch-alias": {
            "dev-develop-2": "2.0.x-dev"
        }
    }
}
